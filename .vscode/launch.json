    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch x64 debug",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/out/x64dbg/server.exe",
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/out/x64dbg",
            "console": "integratedTerminal"
        },
        {
            "name": "Launch x86 debug",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/out/x86dbg/server.exe",
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/out/x86dbg",
            "console": "integratedTerminal"
        }
    ]
}